    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/swagger": "^7.1.6",
